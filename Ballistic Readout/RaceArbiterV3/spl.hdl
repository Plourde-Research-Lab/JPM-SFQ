CIRCUIT spl()
{
PARAMETER
	i1=4.5,
	j1=2.0,
	j2=2.0,
	j3=2.0,
	l1=0.7,
	l2=1.5;
EXTERNAL
	xj1=1.0,
	xj2=1.0,
	xj3=1.0,
	xst=0;

rule init(tcurr<5)
xst=0,
tcurr>5;

rule a1( get(OUT1)  and  xst eq 0)
xst=1,
inc(j1),
set(OUT2),set(IN),
[inc(j2),inc(j3)],
xst=0;

rule a2( get(OUT2)  and xst eq 0)
xst=1,
inc(j2),
set(OUT1),set(IN),
[inc(j1),inc(j3)],
xst=0;

rule a3( get(IN)  and xst eq 0)
xst=1,
inc(j3),
set(OUT1),set(OUT2),
[inc(j1),inc(j2)],
xst=0;

}
